{
	"type": "dark",
	"colors": {
		"editor.background": "#20262c",
		"editor.foreground": "#8cb0d8",
		"editor.selectionBackground": "#424747",
		"editor.selectionHighlightBackground": "#343334",
		"editor.lineHighlightBackground": "#232d38",
		"editorCursor.foreground": "#8cd6d8",
		"editorWhitespace.foreground": "#424747"
	},
	"tokenColors": [
		{
			"name": "Defaults",
			"settings": {
				"foreground": "#f2f4f6"
			}
		},
		{
			"name": "Variables",
			"scope": "source,meta.block,text,variable,support.other.variable,constant.character,constant.other,variable.other.constant,variable.parameter,meta.import,variable.other.readwrite.alias,variable.other.property",
			"settings": {
				"foreground": "#f2f4f6"
			}
		},
		{
			"name": "Punctuation and comments",
			"scope": "meta.property-value,meta.brace,punctuation,comment,constant.character.escape",
			"settings": {
				"foreground": "#627487"
			}
		},
		{
			"name": "Constants",
			"scope": "constant.numeric,support.constant.core",
			"settings": {
				"foreground": "#8cd6d8"
			}
		},
		{
			"name": "Function calls",
			"scope": "entity.name.function,support.function,source.sql",
			"settings": {
				"foreground": "#c6c5eb"
			}
		},
		{
			"name": "Types",
			"scope": "entity.name.type,support.class.builtin,meta.type_params,meta.type.annotation,support.type.primitive,support.type.builtin",
			"settings": {
				"foreground": "#8cb0d8"
			}
		},
		{
			"name": "Type parameters",
			"scope": "meta.type.parameters",
			"settings": {
				"foreground": "#d88e8c"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword,storage,storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#8E8CD8"
			}
		},
		{
			"name": "Attributes and properties",
			"scope": "meta.object-literal.key,meta.structure.dictionary,support.type.property-name,entity.other.attribute-name",
			"settings": {
				"foreground": "#d6d88c"
			}
		},
		{
			"name": "Strings and regular expressions",
			"scope": "string,string.regexp,constant.other.character-class",
			"settings": {
				"foreground": "#b0d88c"
			}
		},
		{
			"name": "String quotes",
			 "scope": "punctuation.definition.string",
			 "settings": {
				  "foreground": "#57862d"
			 }
		},
		{
			"name": "DOM Elements",
			"scope": "entity.name.tag,support.class.component.js",
			"settings": {
				"foreground": "#bf4643"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator,variable.language.this,support.variable.object,support.variable.dom,constant.language,keyword.control.anchor.regexp",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#d88e8c"
			}
		}
	]
}
