{
	"type": "dark",
	"colors": {
		"editor.background": "#20262c",
		"editor.foreground": "#8cb0d8",
		"editor.selectionBackground": "#8cd6d844",
		"editor.selectionHighlightBackground": "#8cd6d822",
		"editor.lineHighlightBackground": "#283037",
		"editorCursor.foreground": "#8cd6d8",
		"editorWhitespace.foreground": "#313943"
	},
	"tokenColors": [
		{
			"name": "Defaults",
			"settings": {
				"foreground": "#f2f4f6"
			}
		},
		{
			"name": "Variables",
			"scope": "source,meta.block,text,variable,support.other.variable,constant.character,constant.other,variable.parameter,meta.import,variable.other.readwrite.alias,variable.other.property,support.variable.property.dom",
			"settings": {
				"foreground": "#f2f4f6"
			}
		},
		{
			"name": "Punctuation and comments",
			"scope": "meta.brace,punctuation,punctuation.section.embedded.end,comment,constant.character.escape,keyword.operator.class",
			"settings": {
				"foreground": "#627487"
			}
		},
		{
			"name": "Constants",
			"scope": "constant.other,constant.numeric,variable.other.constant,support.constant.core,support.constant.property-value,meta.property-value,constant.language,support.variable.dom",
			"settings": {
				"foreground": "#8cd6d8"
			}
		},
		{
			"name": "Function calls",
			"scope": "entity.name.function,support.function,source.sql",
			"settings": {
				"foreground": "#c6c5eb"
			}
		},
		{
			"name": "Types",
			"scope": "entity.name.type,support.class.builtin,meta.type_params,meta.type.annotation,support.type.primitive,support.type.builtin,support.class,support.type.core,meta.class,keyword.other.type",
			"settings": {
				"foreground": "#86b0de"
			}
		},
		{
			"name": "This",
			"scope": "variable.language.this",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#627487"
			}
		},
		{
			"name": "Type parameters",
			"scope": "meta.type.parameters",
			"settings": {
				"foreground": "#d3807d"
			}
		},
		{
			"name": "Keywords",
			"scope": "storage.type,keyword,storage.modifier,storage.type.function,storage.type.class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9c9bdd"
			}
		},
		{
			"name": "Attributes and properties",
			"scope": "meta.object-literal.key,meta.structure.dictionary,support.type.property-name,support.type.vendored.property-name,entity.other.attribute-name",
			"settings": {
				"foreground": "#bd9bdd"
			}
		},
		{
			"name": "Strings and regular expressions",
			"scope": "string,string.regexp,constant.other.character-class,meta.attribute-selector",
			"settings": {
				"foreground": "#a6d37d"
			}
		},
		{
			"name": "String quotes",
			 "scope": "punctuation.definition.string,constant.character.escape",
			 "settings": {
				  "foreground": "#57862d"
			 }
		},
		{
			"name": "DOM Elements",
			"scope": "entity.name.tag,support.class.component.js,entity.other.attribute-name.id.css,entity.other.attribute-name.class.css",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#86b0de"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator,support.variable.object,keyword.control.anchor.regexp",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#d3807d"
			}
		}
	]
}
