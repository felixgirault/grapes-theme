{
	"type": "dark",
	"colors": {
		"editor.background": "#20262c",
		"editor.foreground": "#f2f4f6",
		"editor.selectionBackground": "#8cd6d844",
		"editor.selectionHighlightBackground": "#8cd6d822",
		"editor.lineHighlightBackground": "#283037",
		"editorRuler.foreground": "#313943",
		"editorCursor.foreground": "#8cd6d8",
		"editorWhitespace.foreground": "#313943",
		"panel.background": "#20262c",
		"panel.border": "#627487",
		"sideBar.background": "#20262c",
		"sideBar.foreground": "#d0d6dd"
	},
	"tokenColors": [
		{
			"name": "Defaults",
			"settings": {
				"foreground": "#f2f4f6"
			}
		},
		{
			"name": "Variables",
			"scope": "source,meta.block,text,variable,support.other.variable,constant.character,constant.other,variable.parameter,meta.import,variable.other.readwrite.alias,variable.other.property,support.variable.property.dom,meta.method.body.java,variable.parameter.function.swift",
			"settings": {
				"foreground": "#f2f4f6"
			}
		},
		{
			"name": "Punctuation and comments",
			"scope": "meta.brace,punctuation,punctuation.section.embedded.end,comment,constant.character.escape,keyword.operator.class,keyword.operator.access.dot.rust,meta.parameter-clause.swift",
			"settings": {
				"foreground": "#7b8ea2"
			}
		},
		{
			"name": "Array punctuation",
			"scope": "punctuation.section.array",
			"settings": {
				"foreground": "#9c9bdd"
			}
		},
		{
			"name": "Constants",
			"scope": "constant.other,constant.numeric,variable.other.constant,support.constant.core,support.constant.property-value,meta.property-value,constant.language,support.variable.dom,constant.keyword.clojure,support.constant.swift",
			"settings": {
				"foreground": "#8cd6d8"
			}
		},
		{
			"name": "Function calls",
			"scope": "entity.name.function,support.function,source.sql",
			"settings": {
				"foreground": "#c6c5eb"
			}
		},
		{
			"name": "Types",
			"scope": "storage.type,entity.name.type,support.class.builtin,meta.type_params,meta.type.annotation,support.type.primitive,support.type.builtin,support.class,support.type.core,meta.class,keyword.other.type,meta.function.parameters.php,support.type.swift",
			"settings": {
				"foreground": "#86b0de"
			}
		},
		{
			"name": "This",
			"scope": "variable.language.this,variable.language.super,variable.parameter.function.language.special.self.python,variable.language.special.self.python",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7b8ea2"
			}
		},
		{
			"name": "Type parameters",
			"scope": "meta.type.parameters",
			"settings": {
				"foreground": "#d3807d"
			}
		},
		{
			"name": "Keywords",
			"scope": "storage.type.js,storage.type.ts,storage.type.type.ts,keyword,storage.modifier,storage.type.function,storage.type.class,storage.type.rust,storage.type.struct.swift,storage.type.extension.swift",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9c9bdd"
			}
		},
		{
			"name": "Attributes and properties",
			"scope": "meta.object-literal.key,meta.structure.dictionary,support.type.property-name,support.type.vendored.property-name,entity.other.attribute-name",
			"settings": {
				"foreground": "#bd9bdd"
			}
		},
		{
			"name": "Strings and regular expressions",
			"scope": "string,string.regexp,constant.other.character-class,meta.attribute-selector",
			"settings": {
				"foreground": "#a6d37d"
			}
		},
		{
			"name": "String quotes",
			 "scope": "punctuation.definition.string,constant.character.escape",
			 "settings": {
				  "foreground": "#57862d"
			 }
		},
		{
			"name": "DOM Elements",
			"scope": "entity.name.tag,support.class.component.js,entity.other.attribute-name.id.css,entity.other.attribute-name.class.css",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#86b0de"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator,support.variable.object,keyword.control.anchor.regexp,entity.name.operator.cpp",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#d3807d"
			}
		},
		{
			"name": "Markdown headings",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#86b0de"
			}
		},
		{
			"name": "Markdown punctuation",
			"scope": "markup.underline.link,beginning.punctuation.definition,punctuation.definition.string.begin.markdown,punctuation.definition.string.end.markdown",
			"settings": {
				"foreground": "#7b8ea2"
			}
		},
		{
			"name": "Markdown link URL",
			"scope": "string.other.link.title.markdown",
			"settings": {
				"foreground": "#bd9bdd"
			}
		},
		{
			"name": "Markdown bold",
			"scope": "markup.bold.markdown",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown italic",
			"scope": "markup.italic.markdown",
			"settings": {
				"fontStyle": "italic"
			}
		}
	]
}
